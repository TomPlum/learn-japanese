    "@tomplum/react-textfit": "^1.1.3",
    "bootstrap": "^4.6.0",
    "dayjs": "^1.10.6",
    "deps": "^1.0.0",
    "framer-motion": "^10.16.16",
    "globals": "^13.19.0",
    "i18next": "^23.7.13",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.2",
    "node-sass": "^9.0.0",
    "peer": "^0.6.1",
    "react": "^18.0.0",
    "react-bootstrap": "^1.6.4",
    "react-card-flip": "^1.2.2",
    "react-color": "^2.19.3",
    "react-device-detect": "^2.2.2",
    "react-dom": "^18.0.0",
    "react-grid-layout": "^1.3.0",
    "react-i18next": "^14.0.0",
    "react-icons": "^4.3.1",
    "react-lineto": "^3.3.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^5.3.4",
    "react-scripts": "5.0.1",
    "react-sizeme": "^3.0.2",
    "react-stack-grid": "^0.7.1",
    "react-table": "^7.8.0",
    "react-transition-group": "^4.4.5",
    "recharts": "^2.3.2",
    "redux": "^5.0.1",
    "supermemo": "^2.0.17",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "start:msw": "REACT_APP_ENVIRONMENT=msw react-app-rewired start",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "npm run lint -- --fix",
    "format": "npx prettier --write .",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --reporters jest-silent-reporter",
    "deploy": "npm run build && gh-pages -d build -m \"Release | Deployed Latest Build\"",
    "coverage": "react-app-rewired test --coverage --watchAll",
    "clean": "rm -r node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build",
    "cypress": "cypress open"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx",
      "!src/data/**",
      "!src/types/game/mode/**",
      "!src/types/learn/mode/**",
      "!src/tests/**"
    ],
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "6.1.6",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@types/jest-when": "^3.5.5",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-bootstrap-range-slider": "^1.2.4",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^18.0.0",
    "@types/react-redux": "^7.1.33",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-stack-grid": "^0.7.0",
    "@types/react-table": "^7.7.14",
    "@types/redux-mock-store": "^1.0.3",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "axios-mock-adapter": "^1.20.0",
    "chai-match": "^1.1.1",
    "cypress": "^10.3.0",
    "eslint": "^8.32.0",
    "eslint-plugin-react": "^7.32.1",
    "gh-pages": "^3.1.0",
    "jest-silent-reporter": "^0.5.0",
    "jest-when": "^3.6.0",
    "mq-polyfill": "^1.1.8",
    "msw": "^1.3.2",
    "prettier": "2.8.8",
    "react-app-rewired": "^2.2.1",
    "react-bootstrap-range-slider": "^3.0.8",
    "react-select-event": "^5.3.0",
    "redux-mock-store": "^1.5.4",
    "ts-jest": "^29.1.1"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "overrides": {
    "@tomplum/react-textfit": {
      "react": ">=18.0.0",
      "react-dom": ">=18.0.0"
    },
    "msw": {
      "typescript": "$typescript"
    },
    "react-scripts": {
      "typescript": "5.3.3"
    }
  }
}
